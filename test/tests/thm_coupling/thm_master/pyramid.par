[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 2
  dt = 0.2
  polynomialOrder = 1
  writeControl = steps
  writeInterval = 0

[VELOCITY]
  solver = none
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = false
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = false

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  residualProj  = no
  boundaryTypeMap = f, f, f, f, f, f, f, f
