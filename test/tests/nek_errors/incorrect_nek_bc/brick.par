[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 7
  dt = 5.0e-4
  polynomialOrder = 1
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = false
  boundaryTypeMap = symy, symy, inlet, outlet, symz, symz

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = false

[TEMPERATURE]
  residualTol = 1.0e-5
  residualProj  = no
  boundaryTypeMap = I, I, f, I, I, I
