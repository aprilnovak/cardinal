[GENERAL]
polynomialOrder = 2
stopAt = numSteps
numSteps = 100
dt = 1e-3
timeStepper = tombo2
writeInterval = 100

[MESH]
#boundaryTypeMap = fixedValue,fixedValue,fixedValue,fixedValue,zeroValue,zeroValue
#solver = elasticity
file = "nekbox.re2"

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-4
preconditioner = multigrid
initialGuess = previous

[VELOCITY]
density = 1.0
viscosity = -100.0
boundaryTypeMap = fixedValue,zeroValue,zeroValue,zeroValue,zeroValue,zeroGradient
residualTol = 1e-5

[TEMPERATURE]
solver = none
