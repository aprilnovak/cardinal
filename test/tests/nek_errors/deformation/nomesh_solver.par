[GENERAL]
polynomialOrder = 2
stopAt = numSteps
numSteps = 1
dt = 1e-3
timeStepper = tombo2
writeInterval = 1
oudf = "nekbox.oudf"
udf = "nekbox.udf"

[MESH]
solver = none
file = "nekbox.re2"

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-4

[VELOCITY]
density = 1.0
viscosity = -100.0
boundaryTypeMap = codedFixedValue,zeroValue,zeroValue,zeroValue,zeroValue,zeroGradient
residualTol = 1e-5

[TEMPERATURE]
conductivity = 0.0
rhoCp = 0.0
residualTol = 1.0e-5
boundaryTypeMap = I, I, I, I, I, I
