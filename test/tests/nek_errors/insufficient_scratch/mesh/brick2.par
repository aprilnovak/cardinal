[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1000
  dt = 5.0e-4
  polynomialOrder = 2
  writeControl = steps
  writeInterval = 2
  udf = "../brick.udf"

[PROBLEMTYPE]
  stressFormulation = true

[MESH]
  boundaryTypeMap = zeroValue,codedFixedValue,zeroValue,codedFixedValue,codedFixedValue,codedFixedValue
  solver = pcg+block
  initialGuess = projection+nVector=1
  residualTol = 1e-6
  file = "../brick.re2"

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = symy, symy, codedFixedValue+moving, outlet, symz, symz

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none
