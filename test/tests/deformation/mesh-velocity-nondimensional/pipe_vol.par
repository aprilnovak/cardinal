[GENERAL]
polynomialOrder = 2
stopAt = numSteps
numSteps = 50
dt = 1e-1
timeStepper = tombo2
writeInterval = 50

oudf = "pipe_vol.oudf"
udf = "pipe.udf"

[MESH]
boundaryTypeMap = zeroValue,codedFixedValue,zeroValue
solver = pcg+block
initialGuess = projection+nVector=1
residualTol = 1e-8
file = "pipe.re2"

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-4
preconditioner = multigrid
initialGuess = previous

[VELOCITY]
density = 1.0
viscosity = -100.0
boundaryTypeMap = codedFixedValue,codedFixedValue,zeroGradient
residualTol = 1e-8

[TEMPERATURE]
solver = none
