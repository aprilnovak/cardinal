[GENERAL]
polynomialOrder = 2
stopAt = numSteps
numSteps = 50
dt = 1e-3
timeStepper = tombo2
writeInterval = 50

[MESH]
boundaryTypeMap = zeroValue,codeFixedValue,zeroValue
solver = elasticity
residualTol = 1e-6

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-4
preconditioner = multigrid
initialGuess = previous

[VELOCITY]
density = 1.0
viscosity = -100.0
boundaryTypeMap = codeFixedValue,codeFixedValue,zeroGradient
residualTol = 1e-6

[TEMPERATURE]
solver = none
