[GENERAL]
polynomialOrder = 7
stopAt = endTime
endTime = 0.03
dt = 6.0e-3
timeStepper = tombo2
subCyclingSteps = 2

writeControl = runTime
writeInterval = 1

regularization = hpfrt + nModes=1 + scalingCoeff=10

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = inlet, outlet, wall
density = 1.0
viscosity = 1/19000
residualTol = 1e-06

[CASEDATA]
ZLENGTH = 20.0
NSLABS  = 60
NELSLAB = 108
