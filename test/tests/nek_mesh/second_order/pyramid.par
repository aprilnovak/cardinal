[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 1
  polynomialOrder = 5
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = false
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = false

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  residualProj  = no
  boundaryTypeMap = f, f, f, f, I, I, I, I
