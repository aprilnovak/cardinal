[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 5.0e-4
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 2

[MESH]
  file = ../nek_warnings/no_temp_solve/brick.re2

[VELOCITY]
  solver = none
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = symy, symy, inlet, outlet, symz, symz

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none

[SCALAR01]
  solver = none

[SCALAR02]
  solver = none

[SCALAR03]
  solver = none

